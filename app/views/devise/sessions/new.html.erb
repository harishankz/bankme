<h2>Log in</h2>

<%= bootstrap_form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>

  <%= f.email_field :email %>
  <%= f.password_field :password %>
  <% if devise_mapping.rememberable? %> <%= f.check_box :remember_me %> <% end %>
  <%= f.submit "Log In" %>

<% end %>

<%= render "devise/shared/links" %>
